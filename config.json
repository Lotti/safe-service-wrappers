{
  "stubs": {
    "path": "ide\/%version%\/%namespace%\/",
    "stubs-run-after-generate": false,
    "banner": ""
  },
  "api": {
    "path": "doc\/%version%",
    "theme": {
      "name": "zephir",
      "options": {
        "github": null,
        "analytics": null,
        "main_color": "#3E6496",
        "link_color": "#3E6496",
        "link_hover_color": "#5F9AE7"
      }
    }
  },
  "warnings": {
    "unused-variable": true,
    "unused-variable-external": false,
    "possible-wrong-parameter": true,
    "possible-wrong-parameter-undefined": false,
    "nonexistent-function": true,
    "nonexistent-class": true,
    "non-valid-isset": true,
    "non-array-update": true,
    "non-valid-objectupdate": true,
    "non-valid-fetch": true,
    "invalid-array-index": true,
    "non-array-append": true,
    "invalid-return-type": true,
    "unreachable-code": true,
    "nonexistent-constant": true,
    "not-supported-magic-constant": true,
    "non-valid-decrement": true,
    "non-valid-increment": true,
    "non-valid-clone": true,
    "non-valid-new": true,
    "non-array-access": true,
    "invalid-reference": true,
    "invalid-typeof-comparison": true,
    "conditional-initialization": true
  },
  "optimizations": {
    "static-type-inference": true,
    "static-type-inference-second-pass": true,
    "local-context-pass": true,
    "constant-folding": true,
    "static-constant-class-folding": true,
    "call-gatherer-pass": true,
    "check-invalid-reads": false,
    "internal-call-transformation": false
  },
  "extra": {
    "indent": "spaces",
    "export-classes": false
  },
  "extension-name": "safeservicewrapper",
  "namespace": "safeservicewrapper",
  "name": "safeservicewrapper",
  "description": "Provides service wrappers that get credentials from CyberArk automatically",
  "author": "Valerio Riva",
  "version": "0.0.1",
  "verbose": true,
  "globals": {
    "cyberark.appid": {
      "type": "string",
      "default": "TestAppID",
      "ini-entry": {
        "name": "safeservicewrapper.cyberark.appid",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "cyberark.safe": {
      "type": "string",
      "default": "TestSafe",
      "ini-entry": {
        "name": "safeservicewrapper.cyberark.safe",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "cyberark.folder": {
      "type": "string",
      "default": "TestFolder",
      "ini-entry": {
        "name": "safeservicewrapper.cyberark.folder",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "cyberark.base_url": {
      "type": "string",
      "default": "http://127.0.0.1:3000",
      "ini-entry": {
        "name": "safeservicewrapper.cyberark.base_url",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "cyberark.cert_path": {
      "type": "string",
      "default": "",
      "ini-entry": {
        "name": "safeservicewrapper.cyberark.cert_path",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "cyberark.cert_password": {
      "type": "string",
      "default": "",
      "ini-entry": {
        "name": "safeservicewrapper.cyberark.cert_password",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "curl.timeout": {
      "type": "string",
      "default": 5,
      "ini-entry": {
        "name": "safeservicewrapper.curl.timeout",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "curl.cache_ttl": {
      "type": "string",
      "default": 60,
      "ini-entry": {
        "name": "safeservicewrapper.curl.cache_ttl",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "curl.cache_path": {
      "type": "string",
      "default": "",
      "ini-entry": {
        "name": "safeservicewrapper.curl.cache_path",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_host": {
      "type": "string",
      "default": "127.0.0.1",
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_host",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_port": {
      "type": "string",
      "default": 6379,
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_port",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_user": {
      "type": "string",
      "default": "",
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_user",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_prefix": {
      "type": "string",
      "default": "PHPREDIS_SESSION:",
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_prefix",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.weight": {
      "type": "string",
      "default": 1,
      "ini-entry": {
        "name": "safeservicewrapper.redis.weight",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_timeout": {
      "type": "string",
      "default": 0,
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_timeout",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_database": {
      "type": "string",
      "default": 0,
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_database",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "redis.session_persistent": {
      "type": "string",
      "default": 0,
      "ini-entry": {
        "name": "safeservicewrapper.redis.session_persistent",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "crypto.public_key_path": {
      "type": "string",
      "default": "",
      "ini-entry": {
        "name": "safeservicewrapper.crypto.public_key_path",
        "scope": "PHP_INI_SYSTEM"
      }
    },
    "crypto.private_key_path": {
      "type": "string",
      "default": "",
      "ini-entry": {
        "name": "safeservicewrapper.crypto.private_key_path",
        "scope": "PHP_INI_SYSTEM"
      }
    }
  },
  "ini": {
    "safeservicewrapper.cyberark.appid":"TestAppID",
    "safeservicewrapper.cyberark.safe":"TestSafe",
    "safeservicewrapper.cyberark.folder":"TestFolder",
    "safeservicewrapper.cyberark.base_url":"http://127.0.0.1:3000",
    "safeservicewrapper.cyberark.cert_path":"",
    "safeservicewrapper.cyberark.cert_password": "",
    "safeservicewrapper.curl.timeout": 5,
    "safeservicewrapper.curl.cache_ttl": 60,
    "safeservicewrapper.curl.cache_path": "",
    "safeservicewrapper.redis.session_host": "",
    "safeservicewrapper.redis.session_port": 6379,
    "safeservicewrapper.redis.session_user": "",
    "safeservicewrapper.redis.session_prefix": "PHPREDIS_SESSION:",
    "safeservicewrapper.redis.weight": 1,
    "safeservicewrapper.redis.session_timeout": 0,
    "safeservicewrapper.redis.session_database": 0,
    "safeservicewrapper.redis.session_persistent": 0,
    "safeservicewrapper.crypto.public_key_path": "",
    "safeservicewrapper.crypto.private_key_path": ""
  },
  "info": {
    "some_information": "This is additional information about your extension",
    "another_information": "You can add more information here"
  },
  "requires": {
    "extensions": [
      "redis",
      "curl",
      "json",
      "sodium",
      "pdo_mysql",
      "mysqli"
    ]
  }
}